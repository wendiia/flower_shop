/* /////////////////////////////////////////////////////////////////////////////////////////////////
ComboBox */
QComboBox{
	font: 20pt "Microsoft YaHei UI Light";
	color: #768270;
	font-weight: bold;
	border-radius: 5px;
	border: 2px solid #768270;
	padding-left: 10px;
}
QComboBox:hover{
	border-radius: 5px;
	border: 2px solid #94AF84;
}
QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 25px;
	background-color: #94AF84;
	background-image: url(:/icons/icons/angle-down.png);
	background-position: center;
	background-repeat: no-reperat;
 }
QComboBox QAbstractItemView {
	border-radius: 5px;
	border: 2px solid #94AF84;
	padding: 10px;
	color: #768270;
	selection-background-color: #AAC6A3;
}

