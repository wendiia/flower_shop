}
/* /////////////////////////////////////////////////////////////////////////////////////////////////
DateEdit */
QDateEdit {
	font: 20pt "Microsoft YaHei UI Light";
	color: #768270;
	font-weight: bold;
	padding-left: 5px;
	selection-color: #94AF84;
	border-radius: 5px;
	border: 2px solid #768270;
	selection-background-color: #C0D1B8;
}
QDateEdit::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 25px;
	background-color: #94AF84;
	background-image: url(:/icons/icons/angle-down.png);
	background-position: center;
	background-repeat: no-repeat;
 }
QDateEdit:hover {
	background-color: #94AF84;
	border: 2px solid #94AF84;
}
